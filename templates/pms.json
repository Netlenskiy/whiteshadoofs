{{func}}({
    "type": "FeatureCollection",
    "features": [
    {% for value in bbox %}
        {
                "type": "Feature",
                "id": {{value.id}},
                "geometry": {
                    "type": "Point",
                    "coordinates": [{{value.latitude}}, {{value.longitude}}]
                },
                "icontype": 0,
                "properties": {
                    "balloonContent": "{{value.disclamer}}",
                    "hintContent": "{{value.title}}",
                    "face_link": "{{value.face_link}}",
                    "clusterize": true
                }
        },
    {% endfor %}
    ]
})