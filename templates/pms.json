{{func}}({
    "type": "FeatureCollection",
    "features": [
        {
            {% for value in bbox %}
                "type": "Feature",
                "id": {{value.id}},
                "geometry": {
                    "type": "Point",
                    "coordinates": [{{value.latitude}}, {{value.longitude}}]
                },
                "properties": {
                    "balloonContent": "{{value.disclamer}}",
                    "clusterCaption": "Метка 1",
                    "hintContent": "Текст подсказки"
                }
            {% endfor %}
        }
    ]
})